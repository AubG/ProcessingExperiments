package projectPointFive;

import processing.core.*;

public class flyingWrecks extends collidableObject {
	
	PApplet parent;
	
	flyingWrecks(PApplet p, float x, float y, float w, float h){
		parent = p;
		
		//setting variables
		this.setPosX(200);
		this.setPosY(150);
		this.setWidth(80);
		this.setHeight(35);
	}
	
	boolean isColliding(int p_X, int p_Y){
		//Do something
		return false;
	}
	
	void setup(){
		this.setPosX(200);
		this.setPosY(150);
		this.setWidth(80);
		this.setHeight(35);
	}
	
	@Override
	void display(){
		parent.rect(posX, posY, width, height);
	}
}
